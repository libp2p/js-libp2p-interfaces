  },
  "homepage": "https://github.com/libp2p/interface-transport",
  "devDependencies": {
    "aegir": "^11.0.0",
    "dirty-chai": "^1.2.2"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "multiaddr": "^2.2.2",
    "pull-goodbye": "0.0.1",
    "pull-serializer": "^0.3.2",
    "pull-stream": "^3.5.0"
  },
  "contributors": [
    "David Dias <daviddias.p@gmail.com>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Richard Littauer <richard.littauer@gmail.com>",

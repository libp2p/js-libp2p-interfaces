{
  "name": "interface-transport",
  "version": "0.3.5",
  "description": "A test suite and interface you can use to implement a transport.",
  "repository": {
    "type": "git",
    "url": "https://github.com/libp2p/interface-transport.git"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "aegir-lint",
    "test": "aegir-test",
    "build": "aegir-build",
    "release": "aegir-release --env node",
    "release-minor": "aegir-release --type minor --env node",
    "release-major": "aegir-release --type major --env node",
    "coverage": "exit(0)",
    "coverage-publish": "exit(0)"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
